    name="ux2.personal_tools"
    renderer="templates/personal_tools.pt"
  />

  <view
    for="*"
    view=".views.search"
    name="ux2.search"
    renderer="templates/search.pt"
  />

  <view
    for="*"
    view=".views.context_tools"
    name="ux2.context_tools"
    renderer="templates/context_tools.pt"
  />

  <view
    for="*"
    view=".views.actions_menu"
    name="ux2.actions_menu"
    renderer="templates/actions_menu.pt"
  />

  <view
    for="*"
