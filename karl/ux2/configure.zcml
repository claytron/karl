<configure xmlns="http://pylonshq.com/pyramid">

  <view
    for="*"
    view=".helloworld.helloworld_view"
    name="helloworld.html"
    renderer="templates/helloworld.pt"
  />

  <view
    for="karl.models.interfaces.ICommunities"
    view=".views.communities"
    name="ux2_communities.html"
    renderer="templates/communities.pt"
    permission="view"
  />

  <view
    for="karl.models.interfaces.ICommunity"
    view=".views.community"
    name="ux2_view.html"
    renderer="templates/community.pt"
    permission="view"
  />

  <view
    for="*"
    view=".views.global_nav"
    name="ux2.global_nav"
    renderer="templates/global_nav.pt"
  />

  <view
    for="*"
